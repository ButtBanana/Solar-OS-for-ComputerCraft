shell.run("wget https://raw.githubusercontent.com/Wilma456/Minepack/master/minepack.lua /usr/bin/minepack.lua")
shell.run("wget https://raw.githubusercontent.com/Wilma456/Minepack/master/minepackapi.lua /usr/apis/minepackapi")
shell.run("/usr/bin/minepack.lua fetch")
shell.run("/usr/bin/minepack.lua install minepack")
if tonumber(os.version():sub(8)) > 1.7 then
    shell.run("/usr/bin/minepack.lua install packshell")
else
    shell.run("/usr/bin/minepack.lua install packshell-old")
end
print("All Done! Minepack is now ready!")