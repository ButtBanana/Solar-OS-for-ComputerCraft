term.clear()
term.setCursorPos(1, 1)
print("Cactus OS Arizona")
print("Loading... ... ... ...")
local function BootLog()
  textutils.slowPrint("Press Any Key To Continue At Any Time")
  term.setCursorPos(1, 3)
  textutils.slowPrint("******************")
  textutils.slowPrint("Cactus OS, Designed and Made in Arizona!")
  textutils.slowPrint("Thank you for using Cactus OS.")
  textutils.slowPrint("******************")
  textutils.slowPrint("Now starting Cactus OS...")
end
local function Intercept()
  os.pullEvent("KEY")
end
parallel.waitForAny(BootLog,Intercept)
os.run({},".menu")
